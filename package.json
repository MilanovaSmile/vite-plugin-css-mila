{
  "name": "vite-plugin-css-mila",
  "private": false,
  "version": "2.0.7",
  "description": "Vite plugin to minify CSS",
  "author": "Milana Kustinskaya <milanovasmile@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/MilanovaSmile/vite-plugin-css-mila",
  "type": "module",
  "keywords": [
    "vite",
    "vite-plugin",
    "css",
    "compiler"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/css-mila.umd.js",
  "module": "./dist/css-mila.es.js",
  "exports": {
    ".": {
      "import": "./dist/css-mila.es.js",
      "require": "./dist/css-mila.umd.js"
    }
  },
  "scripts": {
    "dev": "vite build --config dev.vite.config.js",
    "build": "vite build --config build.vite.config.js"
  },
  "devDependencies": {
    "vite": "^5.2.9"
  },
  "peerDependencies": {},
  "dependencies": {
    "clean-css": "^5.3.3",
    "colorette": "^2.0.20"
  }
}
